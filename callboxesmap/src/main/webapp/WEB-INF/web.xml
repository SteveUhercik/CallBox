<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
  
    <servlet>
        <servlet-name>ajaxservlet</servlet-name>
        <servlet-class>cz.muni.fi.pb138.callboxesmap.AjaxServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ajaxservlet</servlet-name>
        <url-pattern>/ajaxservlet/*</url-pattern>
    </servlet-mapping>
  
    <servlet>
        <servlet-name>markersservlet</servlet-name>
        <servlet-class>cz.muni.fi.pb138.callboxesmap.MarkersServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>markersservlet</servlet-name>
        <url-pattern>/markersservlet/*</url-pattern>
    </servlet-mapping>
  
  
</web-app>


